		|| groundquests.isTaskActive(player, "outbreak_quest_rescue_0"+i, "speakSurvivor"))
		{
			groundquests.clearQuest(player, "outbreak_quest_rescue_0"+i); 
			groundquests.grantQuest(player, "outbreak_quest_rescue_0"+i);
			return;
		}
	}
