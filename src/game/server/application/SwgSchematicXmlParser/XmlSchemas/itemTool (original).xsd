										<xs:enumeration value="exp_count"/>
										<xs:enumeration value="exp_damage_max"/>
										<xs:enumeration value="exp_damage_min"/>
										<xs:enumeration value="exp_decel"/>
										<xs:enumeration value="exp_droid_command_speed"/>
